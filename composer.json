{
  "name": "darkin1/intercom",
  "description": "Wrapper on the Intercom class provided by Intercom - with support for Laravel 5.x",
  "keywords": ["laravel", "intercom", "intercom.io", "api", "guzzle"],
  "license": "MIT",
  "authors": [{
    "name": "Dariusz Ciesielski",
    "email": "dciesielski87@gmail.com"
  }],
  "require": {
    "php": ">=7.1",
    "intercom/intercom-php": "^3.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Darkin1\\Intercom\\IntercomServiceProvider"
      ],
      "aliases": {
        "Intercom": "Darkin1\\Intercom\\Facades\\Intercom"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Darkin1\\Intercom\\": "src/"
    }
  },
  "minimum-stability": "dev"
}